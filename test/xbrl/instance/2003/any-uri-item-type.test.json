      "description": "Valid - http URL",
      "data": "http://example.com",
      "valid": true
    },
    {
      "description": "Valid - https URL",
      "data": "https://example.com/path?query=value",
      "valid": true
    },
    {
      "description": "Valid - relative URI",
      "data": "../path/to/resource",
      "valid": true
    },
    {
      "description": "Valid - empty string",
      "data": "",
      "valid": true
    },
    {
      "description": "Invalid - URI with spaces",
      "data": "http://example.com/path with spaces",
      "valid": false
    },
    {
      "description": "Invalid - number instead of string",
      "data": 123,
      "valid": false
    }
