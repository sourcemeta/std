  "x-license": "https://github.com/sourcemeta/std/blob/main/LICENSE",
  "target": "../../../../schemas/xbrl/instance/2003/unsigned-long-item-type.json",
  "tests": [
    {
      "description": "Valid - positive unsigned long",
      "data": "123456789",
      "valid": true
    },
    {
      "description": "Valid - zero",
      "data": "0",
      "valid": true
    },
    {
      "description": "Valid - large unsigned long",
      "data": "18446744073709551615",
      "valid": true
    },
    {
      "description": "Invalid - negative value",
      "data": "-1",
      "valid": false
    },
    {
      "description": "Invalid - decimal value",
      "data": "123.45",
      "valid": false
    },
    {
      "description": "Invalid - number type instead of string",
      "data": 123456789,
      "valid": false
    }
  ]
}
