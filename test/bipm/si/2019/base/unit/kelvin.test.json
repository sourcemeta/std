  "x-license": "https://github.com/sourcemeta/std/blob/main/LICENSE",
  "target": "../../../../../../schemas/2020-12/bipm/si/2019/base/unit/kelvin.json",
  "tests": [
    {
      "description": "Valid - absolute zero",
      "data": 0,
      "valid": true
    },
    {
      "description": "Valid - room temperature",
      "data": 293.15,
      "valid": true
    },
    {
      "description": "Valid - water freezing point",
      "data": 273.15,
      "valid": true
    },
    {
      "description": "Valid - water boiling point",
      "data": 373.15,
      "valid": true
    },
    {
      "description": "Valid - small positive",
      "data": 0.01,
      "valid": true
    },
    {
      "description": "Valid - high temperature",
      "data": 1000,
      "valid": true
    },
    {
      "description": "Invalid - negative (below absolute zero)",
      "data": -1,
      "valid": false
    },
    {
      "description": "Invalid - negative decimal",
      "data": -0.01,
      "valid": false
    },
    {
      "description": "Invalid type - string",
      "data": "273.15",
      "valid": false
    },
    {
      "description": "Invalid type - boolean",
      "data": false,
      "valid": false
    },
    {
      "description": "Invalid type - null",
      "data": null,
      "valid": false
    },
    {
      "description": "Invalid type - array",
      "data": [],
      "valid": false
    },
    {
      "description": "Invalid type - object",
      "data": {},
      "valid": false
    }
  ]
}
