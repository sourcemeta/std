      "data": "2023-05-08:00",
      "valid": true
    },
    {
      "description": "Valid - January",
      "data": "2023-01",
      "valid": true
    },
    {
      "description": "Valid - December",
      "data": "2023-12",
      "valid": true
    },
    {
      "description": "Valid - negative year",
      "data": "-0001-12",
      "valid": true
    },
    {
      "description": "Valid - negative year with timezone",
      "data": "-2023-05Z",
      "valid": true
