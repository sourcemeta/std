    },
    {
      "description": "Invalid - five digit year",
      "data": "10000-05",
      "valid": false
    },
    {
      "description": "Invalid - with day component",
      "data": "2023-05-15",
      "valid": false
    },
    {
      "description": "Invalid - single digit month",
      "data": "2023-5",
      "valid": false
    },
    {
      "description": "Invalid - number type",
      "data": 202305,
      "valid": false
