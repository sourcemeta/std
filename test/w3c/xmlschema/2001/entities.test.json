  "target": "../../../../schemas/w3c/xmlschema/2001/entities.json",
  "tests": [
    {
      "description": "Valid - two entities",
      "data": [ "entity1", "entity2" ],
      "valid": true
    },
    {
      "description": "Valid - single entity",
      "data": [ "singleEntity" ],
      "valid": true
    },
    {
      "description": "Valid - multiple entities",
      "data": [ "_img1", "_img2", "_img3" ],
      "valid": true
    },
    {
