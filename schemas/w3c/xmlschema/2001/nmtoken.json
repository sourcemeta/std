{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "W3C XML Schema NMTOKEN",
  "description": "Represents XML name tokens derived from token (ยง3.3.4)",
  "examples": [
    "abc",
    "123",
    "data-value",
    "element.name",
    "with:colon",
    "_underscore",
    "CamelCase"
  ],
