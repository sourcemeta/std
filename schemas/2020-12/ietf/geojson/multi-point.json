{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RFC 7946 GeoJSON MultiPoint",
  "description": "A geometry object representing multiple positions",
  "examples": [
    {
      "type": "MultiPoint",
      "coordinates": [
        [ 0, 0 ],
        [ 1, 1 ]
      ]
    },
    {
      "type": "MultiPoint",
      "coordinates": [
        [ 0, 0 ]
      ]
    }
  ],
  "x-license": "https://github.com/sourcemeta/std/blob/main/LICENSE",
  "x-links": [ "https://www.rfc-editor.org/rfc/rfc7946#section-3.1.3" ],
  "type": "object",
  "required": [ "type", "coordinates" ],
  "properties": {
    "type": {
      "const": "MultiPoint"
    },
    "coordinates": {
      "type": "array",
      "items": {
        "$ref": "point-coordinates.json"
      }
    },
    "bbox": {
      "$ref": "bounding-box.json"
    },
    "geometry": false,
    "properties": false,
    "features": false
  }
}
