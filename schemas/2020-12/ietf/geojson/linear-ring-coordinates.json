{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RFC 7946 GeoJSON Linear Ring Coordinates",
  "description": "An array of four or more positions representing a closed linear ring, where the first and last positions are equivalent",
  "examples": [
    [
      [ 0, 0 ],
      [ 1, 0 ],
      [ 1, 1 ],
      [ 0, 0 ]
    ],
    [
      [ 0, 0 ],
      [ 1, 0 ],
      [ 1, 1 ],
      [ 0, 1 ],
      [ 0, 0 ]
    ]
  ],
  "x-license": "https://github.com/sourcemeta/std/blob/main/LICENSE",
  "x-links": [ "https://www.rfc-editor.org/rfc/rfc7946#section-3.1.6" ],
  "type": "array",
  "minItems": 4,
  "items": {
    "$ref": "point-coordinates.json"
  }
}
