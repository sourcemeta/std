{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "RFC 7946 GeoJSON MultiPolygon",
  "description": "A geometry object representing multiple Polygon geometries",
  "examples": [
    {
      "type": "MultiPolygon",
      "coordinates": [
        [
          [
            [ 0, 0 ],
            [ 1, 0 ],
            [ 1, 1 ],
            [ 0, 0 ]
          ]
        ]
      ]
    },
    {
      "type": "MultiPolygon",
      "coordinates": [
        [
          [
            [ 0, 0 ],
            [ 1, 0 ],
            [ 1, 1 ],
            [ 0, 0 ]
          ]
        ],
        [
          [
            [ 2, 2 ],
            [ 3, 2 ],
            [ 3, 3 ],
            [ 2, 2 ]
          ]
        ]
      ]
    }
  ],
  "x-license": "https://github.com/sourcemeta/std/blob/main/LICENSE",
  "x-links": [ "https://www.rfc-editor.org/rfc/rfc7946#section-3.1.7" ],
  "type": "object",
  "required": [ "type", "coordinates" ],
  "properties": {
    "type": {
      "const": "MultiPolygon"
    },
    "coordinates": {
      "type": "array",
      "items": {
        "$ref": "polygon-coordinates.json"
      }
    },
    "bbox": {
      "$ref": "bounding-box.json"
    },
    "geometry": false,
    "properties": false,
    "features": false
  }
}
