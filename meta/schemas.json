{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "anyOf": [
    {
      "properties": {
        "examples": {
          "type": "array",
          "minItems": 1
        },
        "const": true
      }
    },
    {
      "properties": {
        "examples": {
          "type": "array",
          "minItems": 3
        },
        "const": false
      }
    }
  ],
  "required": [
    "x-license",
    "title",
    "description",
    "examples",
    "$comment",
    "$schema"
  ],
  "properties": {
    "$schema": {
      "const": "https://json-schema.org/draft/2020-12/schema"
    },
    "$id": false,
    "$comment": {
      "$ref": "../schemas/ietf/http/https-url.json"
    },
    "x-license": {
      "const": "https://github.com/sourcemeta/std/blob/main/LICENSE"
    }
  }
}
