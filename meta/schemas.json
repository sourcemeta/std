{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$dynamicAnchor": "meta",
  "title": "Schema meta-schema",
  "description": "Meta-schema for validating all schemas",
  "examples": [
    {
      "$schema": "https://json-schema.org/draft/2020-12/schema",
      "title": "Example Schema",
      "description": "An example valid schema",
      "type": "string"
    }
  ],
  "$ref": "https://json-schema.org/draft/2020-12/schema",
  "properties": {
    "$id": false,
    "$comment": {
      "$ref": "../schemas/2020-12/misc/single-line-text.json",
      "not": {
        "$ref": "../schemas/2020-12/ietf/uri/uri-reference.json"
      }
    },
    "x-symbol": {
      "type": "string"
    },
    "x-links": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "$ref": "../schemas/2020-12/ietf/http/https-url.json"
      }
    },
    "title": {
      "$ref": "../schemas/2020-12/misc/single-line-text.json"
    },
    "description": {
      "$ref": "../schemas/2020-12/misc/single-line-text.json"
    }
  }
}
